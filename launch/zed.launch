<launch>

  <arg name="svo_file" default=""/>
  <arg name="zed_id" default="0"/>

  <group ns="zed">
    <include file="$(find zed_wrapper)/launch/zed_camera.launch">
      <arg name="svo_file" value="$(arg svo_file)" />
      <arg name="zed_id" value="$(arg zed_id)" />
    </include>
  </group>

  <node pkg="zed_wrapper" type="FilterForRawLinearVelocity" name="FilterForRawLinearVelocity" />
  <!-- <node pkg="zed_wrapper" type="CallStabilizerServiceForLinearVelocity" name="CallStabilizerServiceForLinearVelocity" /> -->

</launch>
